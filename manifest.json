{
    "manifest_version": 2,
    "name": "Magister++",
    "version": "1.0.0",
    "description": "Adds new features to Magister",
    "icons": {
      "256": "Assets/Icons/Icon_256x256.png"
    },
    "content_scripts": [
      {
        "js": ["main.js"],
        "matches": ["https://*.magister.net/*"]
      }
    ],
    "web_accessible_resources": [
      "Styles/*.css"
    ],
    "permissions": ["*://localhost/*"]
  }